<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/student_drawer_layout">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/lightColor"
        tools:context="com.onclick.angie.oneclick_v20.StudentGrade11">

        <include
            layout="@layout/slider_grade11_template" />

    </RelativeLayout>

    <fragment
        android:id="@+id/fragment_student_drawer"
        android:name="com.onclick.angie.oneclick_v20.StudentDrawer"
        android:layout_width="@dimen/nav_drawer"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        app:layout="@layout/fragment_student_drawer"
        tools:layout="@layout/fragment_student_drawer" />
</android.support.v4.widget.DrawerLayout>
